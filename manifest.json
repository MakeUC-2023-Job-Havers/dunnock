{
    "manifest_version": 3,
    "name": "Dunnock",
    "description": "A fact checking Chrome extension.",
    "version": "0.0.1",
    "icons": {
        "128": "./icons/dunnock_128.png"
    },
    "permissions": [
        "sidePanel"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search*"
            ],
            "js": [
                "./scripts/birdReading.js",
                "./scripts/openPanel.js"
            ]
        }
    ],
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "background": {
        "service_worker": "service-worker.js"
    }
    
}